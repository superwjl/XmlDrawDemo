<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tik.xmldrawdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectorActivity" android:screenOrientation="portrait"/>
        <activity android:name=".LayerListActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AnimationActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ShapeActivity" android:screenOrientation="portrait"/>
        <activity android:name=".shape.LineAct" android:screenOrientation="portrait"/>
        <activity android:name=".shape.RectAct" android:screenOrientation="portrait"/>
        <activity android:name=".shape.OvalAct" android:screenOrientation="portrait"/>
        <activity android:name=".shape.RingAct" android:screenOrientation="portrait"/>
    </application>

</manifest>